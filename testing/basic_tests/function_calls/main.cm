typ main :: Int
def main = {
    typ y :: Int;
    def y = 3;
    typ a :: Int;
    def a = add 2 y;
    typ res :: Int;
    def res = if equals y a then 2 else 3;
    res
}

typ add :: Int -> Int -> Int
def add x y =
    x + y

typ equals :: Int -> Int -> Bool
def equals x y =
    if x == y then true else false
