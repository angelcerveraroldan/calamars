typ main :: Int
def main =
    bench 20000 123456789 0

typ bench :: Int -> Int -> Int -> Int
def bench n x acc =
    if n == 0 then acc
    else {
        typ x1 :: Int;
        def x1 = lcg x;
        typ a :: Int;
        def a = x1;
        typ x2 :: Int;
        def x2 = lcg x1;
        typ b :: Int;
        def b = x2;
        typ g :: Int;
        def g = gcd a b;
        bench (n - 1) x2 (acc + g)
    }

typ lcg :: Int -> Int
def lcg x =
    (1103515245 * x + 12345) % 2147483647

typ gcd :: Int -> Int -> Int
def gcd a b =
    if b == 0 then a
    else gcd b (a % b)
